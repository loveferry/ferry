<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.org.ferry.log.mapper.LogLoginMapper">
	<resultMap id="BaseResultMap" type="cn.org.ferry.log.dto.LogLogin">
		<id column="log_id" property="logId" javaType="java.lang.Long" jdbcType="BIGINT"/>
		<result column="user_code" property="userCode" javaType="java.lang.String" jdbcType="VARCHAR"/>
		<result column="login_date" property="loginDate" javaType="java.util.Date" jdbcType="DATE"/>
		<result column="logout_date" property="logoutDate" javaType="java.util.Date" jdbcType="DATE"/>
		<result column="ip" property="ip" javaType="java.lang.String" jdbcType="VARCHAR"/>
		<result column="user_agent" property="userAgent" javaType="java.lang.String" jdbcType="VARCHAR"/>
	</resultMap>

	<insert id="insertLogLogin" parameterType="cn.org.ferry.log.dto.LogLogin" useGeneratedKeys="true" keyProperty="logId" flushCache="true">
		insert into log_login(user_code, login_date, logout_date, ip, user_agent)
		values(#{userCode}, #{loginDate}, #{logoutDate}, #{ip}, #{userAgent})
	</insert>
</mapper>
